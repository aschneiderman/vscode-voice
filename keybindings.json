// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+alt+r",
        "command": "python.execInTerminal"
    },
    {
        "key": "ctrl+alt+k",
        "command": "workbench.action.terminal.clear",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+k",
        "command": "-workbench.action.terminal.clear",
        "when": "terminalFocus"
    }
]